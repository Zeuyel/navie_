# ========================================
# GitHub自动注册系统环境变量配置
# ========================================

# YesCaptcha API配置（验证码解决服务） soft_id 填自己的
YESCAPTCHA_API_KEY=your_yescaptcha_api_key_here
YESCAPTCHA_SOFT_ID=75315

# 浏览器配置
BROWSER_HEADLESS=false
BROWSER_TIMEOUT=30
# ChromeDriver路径（可选，留空则自动搜索默认路径）
CHROME_DRIVER_PATH=

# 用户名生成配置
USERNAME_MIN_LENGTH=6
USERNAME_MAX_LENGTH=20
USERNAME_INCLUDE_NUMBERS=true
USERNAME_INCLUDE_HYPHENS=true
USERNAME_AVOID_CONSECUTIVE_NUMBERS=true
# 可选策略: adjective_noun, word_combination, name_based, tech_style, random_pattern, mixed_style
USERNAME_PREFERRED_STRATEGIES=adjective_noun,word_combination,tech_style

# 日志配置
LOG_LEVEL=INFO

# 闪邮箱配置（邮箱服务提供商）
SHAN_MAIL_ENABLED=true
SHAN_MAIL_CARD_KEY=your_shan_mail_card_key_here
SHAN_MAIL_EMAIL_TYPE=outlook

# WMEmail配置（邮箱服务提供商,还没支持）
WMEMAIL_TOKEN=your_wmemail_token_here

# RoxyBrowser配置（云浏览器服务）
ROXY_API_KEY=your_roxy_api_key_here

# Augment配置（目标注册服务）
AUGMENT_CLIENT_ID=v

# 支付卡信息（用于Augment注册）
CARD_NUMBER=4242424242424242
CARD_EXPIRY=12/25
CARD_CVC=123
BILLING_ADDRESS_LINE1=Kqwjhgfd St
BILLING_ADDRESS_LINE2=Apt Zxcvbn
BILLING_POSTAL_CODE=12345

# 代理配置（可选）
PROXY_ENABLED=false
PROXY_API_URL=https://proxy.scdn.io/api/get_proxy.php
PROXY_PROTOCOL=http
PROXY_POOL_SIZE=10
PROXY_HEALTH_CHECK_INTERVAL=300
PROXY_MAX_FAIL_COUNT=3
